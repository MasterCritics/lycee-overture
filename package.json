{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "concurrently --kill-others \"npm:prettier\" \"npm:development -- --watch\"",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot-files": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-prettier": "onchange 'resources/js/**/*.(js|vue)' -- prettier --write {{changed}}",
    "hot": "concurrently --kill-others \"npm:hot-files\" \"npm:watch-prettier\"",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "axios": "^0.18",
    "bootstrap": "^4.1.2",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "jquery": "^3.2",
    "lodash": "^4.17.4",
    "onchange": "^5.1.0",
    "popper.js": "^1.12",
    "prettier": "^1.15.1",
    "vue": "^2.5.7"
  },
  "dependencies": {
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "element-ui": "^2.4.4",
    "laravel-mix": "^2.1.11",
    "lodash.debounce": "^4.0.8",
    "vue-i18n": "^8.2.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  }
}
